export LANG="en_US.UTF-8"
export LC_ALL="en_US.UTF-8"

if [[ -e $HOME/.virtualenv_profile ]]; then
    source $HOME/.virtualenv_profile
fi

# The next line updates PATH for the Google Cloud SDK.
if [ -f '/Users/dingye/Workspace/google-cloud-sdk/path.bash.inc' ]; then source '/Users/dingye/Workspace/google-cloud-sdk/path.bash.inc'; fi

# The next line enables shell command completion for gcloud.
if [ -f '/Users/dingye/Workspace/google-cloud-sdk/completion.bash.inc' ]; then source '/Users/dingye/Workspace/google-cloud-sdk/completion.bash.inc'; fi
